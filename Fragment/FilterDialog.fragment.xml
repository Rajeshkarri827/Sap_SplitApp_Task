<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">

    <Dialog id="filterDialog"
            title="Filter Orders by Status">

        <VBox id="_IDGenVBox8" class="sapUiSmallMargin">
            <CheckBox id="inTime" text="In time" />
            <CheckBox id="urgent" text="Urgent" />
            <CheckBox id="pending" text="Pending" />
        </VBox>

        <endButton>
            <Button id="_IDGenButton" text="Apply" press=".onApplyFilter" />
        </endButton>
        <beginButton>
            <Button id="_IDGenButton1" text="Cancel" press=".onCancelFilter" />
        </beginButton>
    </Dialog>

</core:FragmentDefinition>
