<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="filterDialog" title="Filter Orders">
        <Button id="btnClearFilter1" text="clear Fields" icon="sap-icon://reset" press=".onClearFilter" />

        <VBox id="_IDGenVBox7" class="sapUiSmallMargin">

            <Label id="_IDGenLabel" text="Enter Order ID" />
            <Input id="orderIdInput" 
                   showValueHelp="true" 
                   valueHelpRequest=".onOrderIdValueHelp" />

            <Label id="_IDGenLabel1" text="Enter Customer Name" />
            <Input id="customerNameInput" 
                   showValueHelp="true" 
                   valueHelpRequest=".onCustomerNameValueHelp" />

            <Label id="_IDGenLabel2" text="Enter Category" />
            <Input id="categoryInput" 
                   showValueHelp="true" 
                   valueHelpRequest=".onCategoryValueHelp" />

        </VBox>
       
        
        

        <beginButton>
            <Button id="_IDGenButton1" text="Apply" press=".onApply" />
        </beginButton>
        
        <endButton>
            <Button id="_IDGenButton3" text="Cancel" press=".onCancel" />
        </endButton>

    </Dialog>
</core:FragmentDefinition>
